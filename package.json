{
	"name": "natours",
	"version": "1.0.0",
	"description": "learning node express and mongDB",
	"main": "server.js",
	"targets": {
		"main": false
	},
	"scripts": {
		"start": "node server.js",
		"dev": "nodemon server.js",
		"production": "NODE_ENV=production nodemon server.js",
		"showDatabaseExplanation": "NODE_ENV=DBExplain nodemon server.js",
		"import": "node ./dev-data/data/import-dev-data.js --import",
		"delete": "node ./dev-data/data/import-dev-data.js --delete",
		"debug": "sudo ndb server.js",
		"deleteUser": "node ./dev-data/data/import-dev-data.js --deleteUser",
		"importUser": "node ./dev-data/data/import-dev-data.js --importUser",
		"importReview": "node ./dev-data/data/import-dev-data.js --importReview",
		"deleteReview": "node ./dev-data/data/import-dev-data.js --deleteReview",
		"updateRepo": "node ./dev-data/data/import-dev-data.js --updateRepo",
		"deleteRepo": "node ./dev-data/data/import-dev-data.js --deleteRepo",
		"watch:js": "parcel watch ./public/js/index.js --dist-dir ./public/js/bundled",
		"build:js": "parcel build ./public/js/index.js --dist-dir ./public/js/bundled"
	},
	"author": "Chenxin Gao",
	"license": "ISC",
	"dependencies": {
		"axios": "^1.6.7",
		"bcryptjs": "^2.4.3",
		"compression": "^1.7.4",
		"cookie-parser": "^1.4.6",
		"core-js": "^3.36.0",
		"dotenv": "^16.3.1",
		"express": "^4.18.2",
		"express-mongo-sanitize": "^2.2.0",
		"express-rate-limit": "^7.1.5",
		"helmet": "^7.1.0",
		"hpp": "^0.2.3",
		"html-to-text": "^9.0.5",
		"jsonwebtoken": "^9.0.2",
		"mongodb": "^6.3.0",
		"mongoose": "^8.1.0",
		"morgan": "^1.10.0",
		"multer": "^1.4.5-lts.1",
		"nodemailer": "^6.9.9",
		"pug": "^3.0.2",
		"regenerator-runtime": "^0.14.1",
		"sharp": "^0.33.2",
		"slugify": "^1.6.6",
		"validator": "^13.11.0",
		"xss-clean": "^0.1.4"
	},
	"devDependencies": {
		"@types/node": "^20.11.5",
		"buffer": "^6.0.3",
		"cross-env": "^7.0.3",
		"eslint": "^8.56.0",
		"eslint-config-airbnb": "^19.0.4",
		"eslint-config-prettier": "^9.1.0",
		"eslint-plugin-import": "^2.29.1",
		"eslint-plugin-jsx-a11y": "^6.8.0",
		"eslint-plugin-node": "^11.1.0",
		"eslint-plugin-prettier": "^5.1.3",
		"eslint-plugin-react": "^7.33.2",
		"ndb": "^0.2.4",
		"parcel": "^2.11.0",
		"prettier": "^3.2.4"
	},
	"engines": {
		"node": ">=20.3.0"
	},
	"browserslist": {
		"production": [
			">0.2%",
			"not dead",
			"not op_mini all"
		],
		"development": [
			"last 1 chrome version",
			"last 1 firefox version",
			"last 1 safari version"
		]
	}
}
